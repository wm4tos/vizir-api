{
  "extends": "airbnb/base",
  "env": {
    "es6": true,
    "node": true,
    "mongo": true
  },
  "plugins": [
    "import"
  ],
  "rules": {
    "max-len": ["error", 120],
    "no-underscore-dangle": ["off"],
    "no-param-reassign": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "no-return-assign": 0,
    "prefer-promise-reject-errors": 0,
    "no-restricted-syntax": 0
  }
}
